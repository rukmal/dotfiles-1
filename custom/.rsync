rsyncdest="csu@attu.cs.washington.edu"

function rpull() {
    localdir=$(pwd)
    remotedir="${localdir/\/Users\/christophersu//homes/iws/csu}"
    rsync -azP --delete "$rsyncdest:$remotedir" "$localdir"
}

function rpush() {
    localdir=$(pwd)
    remotedir="${localdir/\/Users\/christophersu//homes/iws/csu}"
    rsync -azP --delete "$localdir" "$rsyncdest:$remotedir"
}